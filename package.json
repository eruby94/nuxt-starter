{
  "name": "nuxt-starter",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Evan Ruby <evanruby94@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@nuxtjs/apollo": "^2.1.1",
    "apollo-client": "^1.9.3",
    "graphql-tag": "^2.4.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "nuxt": "^1.0.0-rc11",
    "vue-apollo": "^2.1.0-rc.5",
    "vue-localstorage": "^0.4.2",
    "vuedraggable": "^2.14.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
